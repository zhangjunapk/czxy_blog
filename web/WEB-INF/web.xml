    <servlet-class>org.tean8.blog.controller.ArticleController</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>/ArticleController</servlet-name>
        <url-pattern>/ArticleController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>/TypeTagServlet</servlet-name>
