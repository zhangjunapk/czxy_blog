    </servlet>
    <servlet-mapping>
        <servlet-name>/JsonController</servlet-name>
        <url-pattern>/JsonController</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>/LinkController</servlet-name>
        <servlet-class>org.tean8.blog.controller.LinkController</servlet-class>
    </servlet>
    <servlet-mapping>
